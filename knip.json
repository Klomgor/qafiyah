{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "tags": ["-lintignore"],
  "workspaces": {
    ".": {},
    "apps/web": {
      "entry": ["src/app/**/*.{ts,tsx}", "next.config.*"],
      "project": ["src/**/*.{ts,tsx}"],
      "ignoreDependencies": ["@qaf/zod-schemas"]
    },
    "apps/api": {
      "entry": "src/app.ts",
      "project": "src/**/*.ts"
    },
    "packages/zod-schemas": {
      "entry": "src/**/*.ts",
      "project": "src/**/*.ts"
    }
  }
}
